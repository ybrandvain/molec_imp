	Mesh = {Animals; Epistasis, Genetic; Female; Genome; Genomic Imprinting; Inbreeding; Male; Mice; Mice, Inbred Strains; Models, Genetic; Pedigree; Quantitative Trait Loci},
	Num-Scopus-Cites = {7},
