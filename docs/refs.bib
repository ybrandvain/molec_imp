	Journal = {PLoS Genet},
	Month = mar,
	Num-Scopus-Cites = {111},
	Number = {3},
	Pages = {e41},
	Title = {Assumption-Free Estimation of Heritability from Genome-Wide Identity-by-Descent Sharing between Full Siblings},
	Url = {http://dx.plos.org/10.1371/journal.pgen.0020041},
	Volume = {2},
