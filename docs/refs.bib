	Issn = {1469-1809},
	Journal = {Ann Hum Genet},
	Language = {en},
	Num-Scopus-Cites = {50},
	Number = {6},
	Pages = {583–601},
	Title = {Power of regression and maximum likelihood methods to map QTL from sib-pair and DZ twin data},
	Url = {http://onlinelibrary.wiley.com/doi/10.1046/j.1469-1809.2001.6560583.x/abstract},
